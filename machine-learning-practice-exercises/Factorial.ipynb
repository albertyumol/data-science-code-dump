    "import numpy as np\n",
    "\n",
    "def f(n):\n",
    "    factorial = 1\n",
    "    if n < 0:\n",
    "       print('No factorial')\n",
    "    elif n == 0:\n",
    "       print('1')\n",
    "    else:\n",
    "        for i in range(1, n + 1):\n",
    "            factorial = factorial*i\n",
    "        print(factorial)\n",
    "    \n",
    "f(5)"
   ]
